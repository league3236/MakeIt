CREATE TABLE 'competition' (
Ctitle varchar(100) NOT NULL,
Carea varchar(30) NOT NULL,
Chost varchar(30),
Cinstitute varchar(30),
Cperiod varchar(30),
Ctarget varchar(255),
Clink varchar(255),
primary key(Ctitle),
);
