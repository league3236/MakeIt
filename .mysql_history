CREATE TABLE competition (
C_id INT NOT NULL AUTO_INCREMENT PRIMARY KEY, 
C_title varchar(100) NOT NULL,
C_area varchar(30) NOT NULL,
C_host varchar(30),
C_institute varchar(30),
C_period varchar(30),
C_target varchar(255),
C_link varchar(255),
C_all text
);
